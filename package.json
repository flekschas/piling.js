{
  "name": "piling.js",
  "version": "0.5.0",
  "description": "A WebGL-based Library for Visual Piling/Stacking",
  "author": [
    {
      "name": "Fritz Lekschas",
      "url": "https://lekschas.de"
    },
    {
      "name": "Xinyi Zhou"
    }
  ],
  "keywords": [
    "piling",
    "stacking",
    "visualization",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/piling.js",
  "module": "dist/piling.esm.js",
  "files": [
    "src/*",
    "dist/piling.js",
    "dist/piling.min.js",
    "dist/piling.esm.js",
    "dist/piling-library.js",
    "dist/piling-library.min.js",
    "dist/piling-library.esm.js",
    "dist/piling-renderer.js",
    "dist/piling-renderer.min.js",
    "dist/piling-renderer.esm.js",
    "dist/piling-aggregator.js",
    "dist/piling-aggregator.min.js",
    "dist/piling-aggregator.esm.js"
  ],
  "scripts": {
    "build": "rm -rf dist/*; rm -rf examples-build/*; rollup -c; webpack --config ./webpack.config.js --mode production; ./copy-example-data.sh; echo \"piling.js.org\" >> examples-build/CNAME",
    "deploy": "npm run build; gh-pages -d examples-build -t --git git",
    "lint": "eslint src examples rollup.config.js webpack.config.js",
    "precommit": "NODE_ENV=production lint-staged",
    "prepublishOnly": "npm run test",
    "prerelease": "npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "test": "rollup -c; webpack --config ./webpack.config.js",
    "watch": "rollup -cw",
    "wbuild": "webpack --config ./webpack.config.js --mode development"
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@flekschas/utils": "~0.12.0",
    "camera-2d-simple": "~2.0.1",
    "deep-equal": "~1.1.0",
    "dom-2d-camera": "~1.0.0-rc.6",
    "gl-matrix": "~3.1.0",
    "liang-barsky": "~1.0.4",
    "ndarray": "~1.0.18",
    "ndarray-convolve": "~2.0.0",
    "ndjson-to-json-text": "^1.0.2",
    "normalize-wheel": "~1.0.1",
    "pixi.js": "~5.1.6",
    "pub-sub-es": "~1.2.1",
    "rbush": "~3.0.1",
    "redux": "~4.0.5",
    "redux-batched-actions": "~0.4.1",
    "redux-freeze": "~0.1.7",
    "regl": "~1.3.13",
    "style-object-to-css-string": "~1.0.1",
    "umap-js": "~1.3.2",
    "with-raf": "~1.1.1"
  },
  "peerDependencies": {
    "pub-sub-es": "~1.2.1",
    "pixi.js": "~5.1.6",
    "regl": "~1.3.13",
    "umap-js": "~1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "acorn": "^7.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "browser-env": "^3.2.5",
    "d3-scale-chromatic": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "esm": "^3.2.25",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-visualizer": "^3.3.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "homepage": "https://github.com/flekschas/piling.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/piling.js.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/piling.js/issues"
  }
}
